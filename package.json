{
  "name": "cimplex-configurator",
  "version": "1.0.0",
  "description": "A interactive cimplex configurator for customizable visframework installations",
  "scripts": {
    "start": "node server.js",
    "watch": "watchify src/index.js -o public/dist/app.js -v",
    "install": "cd ./installer && npm install",
    "postinstall": "ncp ./patches/fabricator.js ./node_modules/pkg/lib-es5/fabricator.js"
  },
  "author": "German Research Center for Artificial Intelligence",
  "license": "Apache-2.0",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "sourceMaps": true,
          "sourceMapsAbsolute": true
        }
      ]
    ]
  },
  "babel": {
    "presets": [
      "react"
    ]
  },
  "dependencies": {
    "file-saver": "^1.3.3",
    "finalhandler": "^1.0.3",
    "fs-walk": "0.0.1",
    "material-ui": "^0.18.6",
    "ncp": "^2.0.0",
    "pkg": "^4.2.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-tap-event-plugin": "^2.0.1",
    "resourcehacker": "^4.2.51",
    "rimraf": "^2.6.1",
    "serve-static": "^1.12.3",
    "ua-parser-js": "^0.7.14",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "watchify": "^3.9.0"
  }
}
